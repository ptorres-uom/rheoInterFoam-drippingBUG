FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
      type            fixedValue;
      value           uniform (0 -1.66 0);
    }
    
    outlet
    {
       type            zeroGradient;
    }

    atmosphere
    {
       type            zeroGradient;
    }
    
    wall_wet
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
    
    wall_no_wet
    {
      type            fixedValue;
      value           uniform (0 0 0);
    }
    
    wFront
    {
       type            wedge;
    }

    wBack
    {
       type            wedge;
    }

    axis
    {
       type            empty;
    }
    
}
